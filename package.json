{
  "name": "@aderesh/trie",
  "version": "0.0.1",
  "description": "A simple typed trie implementation in TS. Ideal for autocomplete or a spell check.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion" : "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aderesh/trie.git"
  },
  "keywords": [
    "trie",
    "autocomplete",
    "spell-check"
  ],
  "author": "Andrii Deresh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aderesh/trie/issues"
  },
  "homepage": "https://github.com/aderesh/trie#readme",
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "jest": "^29.3.1",
    "prettier": "^2.8.2",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.4"
  },
  "files": [
    "lib/**/*"
  ]
}